{
    "plugins": ["prettier-plugin-tailwindcss"],
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "arrowParens": "avoid",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "semi": true,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }